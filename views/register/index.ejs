<form action="" method="POST" id="form_create_user">
    <div class="block_login">
        <h1 style="margin: 50px"><b>Đăng Ký</b></h1>
        <%if (error) {%>
            <div class="alert alert-danger" role="alert">
                <strong>No no no ::)!</strong>  Tài khoản đã tồn tại
            </div>
        <%}%>
        <div class="form-group">
            <input type="hidden" name="id">
            <input type="hidden" name="_csrf" id="_csrf" value="<%= csrfToken%>">
            <input type="text" name="account" class="form-control" required placeholder="Tài khoản" >
            <input type="text" name="name" class="form-control" required placeholder="Tên" >
            <input type="password" name="password" class="form-control" minlength="5" required placeholder="Mật khẩu" >
            <button type="submit" class="btn btn-danger">Đăng ký</button>
        </div>
    </div>
</form>